{
    "name": "__MSG_extName__",
    "version": "17.3.15",
    "author": "Web eID team",
    "homepage_url": "https://web-eid.com",
    "manifest_version": 2,
    "description": "__MSG_extDescription__",
    "default_locale": "en",
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [{
        "matches": ["*://*/*", "file:///*"],
        "js": ["content.js"],
        "run_at": "document_end",
        "all_frames": true
    }],
    "background": {
        "scripts": ["background.js"],
        "permanent": true
    },
    "browser_action": {
        "default_title": "Web eID options",
        "default_icon": {
            "48": "icon48.png"
        }
    },
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "permissions": [
        "https://updates.web-eid.com/",
        "nativeMessaging"
    ],
    "web_accessible_resources": [
        "legacy.js"
    ],
    "applications": {
        "gecko": {
            "id": "{75323b42-f502-11e6-9fd0-6cf049ee125a}",
            "strict_min_version": "52.0",
            "update_url": "https://updates.web-eid.com/firefox.json"
        }
    }
}
